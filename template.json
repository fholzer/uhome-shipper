{
  "index_patterns": [
    "sensors-*"
  ],
  "mappings": {
    "doc": {
      "dynamic_templates": [
        {
          "strings_as_keyword": {
            "mapping": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "match_mapping_type": "string"
          }
        }
      ],
      "properties": {
        "@timestamp": {
          "type": "date",
          "format": "yyyy-MM-dd' 'HH:mm:ss"
        }
      }
    }
  }
}
